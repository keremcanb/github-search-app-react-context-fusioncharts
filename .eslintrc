{
  "extends": ["wesbos"],
  "rules": {
    "react/jsx-props-no-spreading": 0,
    "camelcase": 0,
    "react/button-has-type": 0,
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "import/named": 0,
    "no-unused-vars": 0,
    "default-case": 0,
    "react-hooks/exhaustive-deps": 0,
    "react/no-danger": 0,
    "react/prop-types": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "none",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
